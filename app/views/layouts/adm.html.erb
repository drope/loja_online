<!DOCTYPE html>
<html>
<head>
  <title>LojaOnline</title>

  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	
	<ul> 
		
		<% if signed_in? %> 
		<li><%= link_to "Sign out", logout_path, :method => :delete %></li> 				  
		<% else %> 
		<li><%= link_to "Sign in", login_path %></li> 
		<% end %>
	</ul>
	

	<section class="round">
	<% flash.each do |key, value| %> <div class="flash <%= key %>"><%= value %></div>
	<% end %>
	<%= yield %> </section>



<%= debug(params) if Rails.env.development? %>

</body>
</html>
